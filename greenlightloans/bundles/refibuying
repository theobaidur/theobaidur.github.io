function checkIfBuyRefiPage(){var t=location.pathname,i=location.hash,n=t+i;n==="/buying#hero-form"&&window.scrollTo(0,235);n==="/refinancing#hero-form"&&window.scrollTo(0,235)}function checkIfMblBuyRefi(){var r=location.pathname,n=[],i,t,u,f;if(r==="/buying/mobile"||r==="/refinancing/mobile"){if(n.length==0&&window.location.search.split("?").length>1)for(i=window.location.search.split("?")[1].split("&"),t=0;t<i.length;t++)u=i[t].split("=")[0],f=decodeURIComponent(i[t].split("=")[1]),n[u]=f;if(n.LoanType!=null&&n.HomeType!=null&&n.PropState!=null){var e=n.LoanType,o=n.HomeType,s=n.PropState;$("#hdnLoanType").text(e);$("#hdnHomeType").text(o);$("#hdnPropState").text(s)}}}(function(){checkIfBuyRefiPage();checkIfMblBuyRefi()})()